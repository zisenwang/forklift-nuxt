generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Electric_Order_Pickers {
  id               BigInt    @id(map: "Electric_Order_Picker_pkey") @default(autoincrement())
  created_at       DateTime? @default(now()) @db.Timestamptz(6)
  model            String    @unique(map: "Electric_Order_Picker_model_key")
  description      String?
  capacity_kg      Int
  lift_height_mm   Int?
  fork_size        String?
  battery_voltage  Int?      @db.SmallInt
  battery_ampere   Int?      @db.SmallInt
  battery_type     String?
  controller_type  String?
  travel_speed_kmh Float?    @db.Real
  rrp              Int
  notes            String[]
  optionals        Json[]
}

model Electric_Pallet_Trucks {
  id               BigInt    @id @default(autoincrement())
  created_at       DateTime? @default(now()) @db.Timestamptz(6)
  model            String    @unique
  description      String?
  capacity_kg      Int
  lift_height_mm   Int?
  fork_size        String?
  battery_voltage  Int?      @db.SmallInt
  battery_ampere   Int?      @db.SmallInt
  battery_type     String?
  controller_type  String?
  travel_speed_kmh Float?    @db.Real
  rrp              Int
  notes            String[]
  optionals        Json[]
}

model Electric_Stackers {
  id               BigInt    @id(map: "Electric_Stacker_pkey") @default(autoincrement())
  created_at       DateTime? @default(now()) @db.Timestamptz(6)
  model            String    @unique(map: "Electric_Stacker_model_key")
  description      String?
  capacity_kg      Int
  lift_height_mm   Int?
  fork_size        String?
  battery_voltage  Int?      @db.SmallInt
  battery_ampere   Int?      @db.SmallInt
  battery_type     String?
  controller_type  String?
  travel_speed_kmh Float?    @db.Real
  rrp              Int
  notes            String[]
  optionals        Json[]
}

model Forklifts {
  id               BigInt    @id @default(autoincrement())
  created_at       DateTime? @default(now()) @db.Timestamptz(6)
  model            String    @unique
  description      String?
  capacity_kg      Int
  lift_height_mm   Int?
  fork_size        String?
  battery_voltage  Int?      @db.SmallInt
  battery_ampere   Int?      @db.SmallInt
  battery_type     String?
  controller_type  String?
  travel_speed_kmh Float?    @db.Real
  rrp              Int
  notes            String[]
  optionals        Json[]
}

model Users {
  id         BigInt    @id @default(autoincrement())
  created_at DateTime? @default(now()) @db.Timestamptz(6)
  username   String    @unique @db.VarChar
  password   String    @db.VarChar
}
